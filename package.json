{
    "name": "jsmacros",
    "displayName": "jsmacros",
    "description": "Run Javascript macros to transform the content of documents in VS Code.",
    "repository": "https://github.com/eduardoportilho/jsmacros",
    "version": "0.0.1",
    "publisher": "eduardoportilho",
    "engines": {
        "vscode": "^1.25.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.jsmacros"
    ],
    "main": "./extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.jsmacros",
                "title": "JS macros"
            }
        ]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "eslint": "^4.11.0",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    }
}
